public static Integer findLargest(List<Integer> nums) {
	//code here
    Integer i, tempLargeNum;
    for (i=0;i<nums.size()-1;i++){
        if(nums.get(i)>nums.get(i+1)){
            tempLargeNum = nums.get(i);
        }
        else if(nums.get(i)<nums.get(i+1)){
             tempLargeNum = nums.get(i+1);
        }
    }
    return tempLargeNum;
}