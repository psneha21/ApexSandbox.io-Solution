public static Integer findLargest(List<Integer> nums) {
	//code here
    Integer i = 0, tempLargeNum = nums.get(i);
    for (i=0;i<nums.size()-1;i++){
        if(tempLargeNum < nums.get(i+1)){
            tempLargeNum = nums.get(i+1);
        }
        else{
             tempLargeNum = tempLargeNum;
        }
    }
    return tempLargeNum;
}